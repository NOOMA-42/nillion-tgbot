{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "public",
    "lib": ["ESNext", "DOM"],
    "resolveJsonModule": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}